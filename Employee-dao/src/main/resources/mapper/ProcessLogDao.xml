<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
 	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dianke.employee.dao.ProcessLogDao">
	<insert id="save" parameterType="com.dianke.employee.domain.ProcessLog">
		insert into process_log(user_name,user_id,pro_msg,create_time) values(#{username},#{userId},#{proMsg},#{createTime})
	</insert>
</mapper>